 
public class Part3 {
	public String dna;
	public Part3(String dna){
		this.dna=dna;
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Part3 pr=new Part3("adfaffa");
		System.out.println(pr.count(""));
	}
	public int findstopCodon(String dna,int i, String stopCodon){
		int a=i;
		int t=dna.indexOf(stopCodon,a);
		int c=0;
		int max=0;
		while(t!=-1){
			a=t;
			if((t-i)%3==0 ){
				c+=1;
			}
			t=dna.indexOf(stopCodon,a+1);
		}
		return c;
	}
	public int count(String dna){
		int ind2=0;
		int count=0;
		String[] k={"TAG"};
		ind2=dna.indexOf("ATG",ind2+1);
		while(ind2!=-1){
			for(String j:k){
				count+=findstopCodon(dna,ind2, j);
			}
			ind2=dna.indexOf("ATG",ind2+1);
		}
		return count;
		
		
	}

}
